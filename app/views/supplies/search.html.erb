




 <%= form_tag(search_path, :method => "get") do %>  
  
    <%= search_field_tag :search, params[:search], placeholder: "Search", class: "form-control" %>  
   
      <%= button_tag "", :class => 'btn btn-info glyphicon glyphicon-search',:Name => nil%> 

<% end %>


<h3>Search Result</h3>  

    <% @results.each do |result| %>  
        <%= result.Name %><br>  
    <% end %>